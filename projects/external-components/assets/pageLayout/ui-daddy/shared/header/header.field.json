{
    "header-dashboard": [
        {
            "value": "UI Daddy",
            "baseProperties": {
                "name": "menu",
                "type": "staticText"
            },
            "layoutProperties": {
                "css": {
                    "configuration": {
                        "selectorName": ".header__logo",
                        "xs": {
                            "min-height": "40px",
                            "font-size": "30px",
                            "margin-left": "15px",
                            "parent-justify-content": "flex-start",
                            "parent-padding": "8px"
                        }
                    },
                    "customClassName": "header__logo"
                }
            }
        },
        {
            "baseProperties": {
                "name": "userName",
                "type": "staticText"
            },
            "value": "Welcome {0}",
            "dynamicValueAccessor": [
                "name"
            ],
            "layoutProperties": {
                "css": {
                    "configuration": {
                        "selectorName": ".header__user-name",
                        "xs": {
                            "padding-right": "40px",
                            "parent-justify-content": "flex-end"
                        }
                    },
                    "customClassName": "header__user-name hide-xs"
                }
            }
        },
        {
            "baseProperties": {
                "name": "logout",
                "label": "Log Out",
                "type": "submit",
                "id": "logout"
            },
            "layoutProperties": {
                "css": {
                    "configuration": {
                        "selectorName": ".header-common__logout",
                        "xs": {
                            "parent-white-space": "nowrap",
                            "parent-justify-content": "flex-end",
                            "background-color": "#222121",
                            "border-color": "#222121"
                        }
                    },
                    "customClassName": "header-common__logout",
                    "themeClass": "btn btn-dark"
                }
            },
            "events": [
                {
                    "event": "click",
                    "actions": [
                        {
                            "actionType": "INVOKE_API",
                            "showLoading": true,
                            "apiConfig": {
                                "name": "signupButtonApi",
                                "url": "/rest/logout",
                                "method": "POST",
                                "payloadParams": [],
                                "onSuccess": {
                                    "pageUrl": "/login",
                                    "clearSharedData": [
                                        "authenticated",
                                        "name"
                                    ]
                                }
                            }
                        }
                    ],
                    "debounce": 3000
                }
            ]
        }
    ]
}